<!---Main Content Start--->
<div class="ms_content_wrapper padder_top80">
    <!---Header--->
    <div class="ms_header">
        <div class="ms_top_left">
            <form action="/Home/Index" method="GET">
                <div class="ms_top_search">
                    <input type="text" class="form-control" name="nameMusicSearch" placeholder="Search Music Here..">
                    <button type="submit" class="search_icon">
                        <img img src="~/images/svg/search.svg" alt="">
                    </button>
                </div>
            </form>
            <div class="ms_top_trend">
                <span><a href="#" class="ms_color">Trending Songs :</a></span> <span class="top_marquee"><a href="#">Dream your moments, Until I Met You</a></span>
            </div>
        </div>
        <div class="ms_top_right">
            
            <div class="ms_top_btn">
                @if (!string.IsNullOrEmpty(UserName))
                {
                    <a href="/Home/ProFile" class="ms_btn_Hello login_btn"><span>Hello: @UserName</span></a>
                    <a href="/Message/MessageHome" class="ms_btn login_btn"><span>Message</span></a>
                    if (IsAdmin == true)
                    {
                        <a href="/Admin/Home" class="ms_btn login_btn"><span>Page Admin</span></a>
                    }
                    <a href="/Home/Login" class="ms_btn login_btn"><span>logout</span></a> 
                }else{
                    <a href="/Home/Register_Here" class="ms_btn reg_btn"><span>register</span></a>
                    <a href="/Home/Login" class="ms_btn login_btn"><span>login</span></a>
                }
            </div>
        </div>
    </div>
    <!---Banner--->
    <div class="ms-banner">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12">
