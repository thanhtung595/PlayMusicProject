@{
    string UserName = "";
    bool IsAdmin = false;
    if (TempData["IdUser"] != null)
    {
        int IdUser = (int)TempData["IdUser"];
        UserName = (string)TempData["UserName"];
        string AccountUser = (string)TempData["AccountUser"];
        string AccountPass = (string)TempData["AccountPass"];
        string UserImage = (string)TempData["UserImage"];
        IsAdmin = (bool)TempData["CheckAdmin"];
    }
}
